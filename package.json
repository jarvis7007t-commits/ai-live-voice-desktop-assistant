
{
  "name": "gemini-live-vision-desktop",
  "private": true,
  "version": "1.0.0",
  "description": "Desktop AI Assistant with Live Vision & Voice",
  "author": "AI Developer",
  "main": "electron.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "react-dom": "^19.2.3",
    "@google/genai": "^1.38.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.gemini.livevision",
    "productName": "Gemini Live Vision",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "https://cdn-icons-png.flaticon.com/512/2099/2099165.png"
    },
    "files": [
      "dist/**/*",
      "electron.js",
      "package.json"
    ]
  }
}
